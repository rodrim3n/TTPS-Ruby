<h1>Cliente <%= @client.name %> </h1>

<ul>
  <li><%= @client.lastname %></li>
  <li><%= @client.dni %></li>
  <li><%= @client.birthdate %></li>
  <li><%= @client.gender %></li>
  <li>Contactos:
    <% @client.contacts.each do |contact| %>
      <%= contact.type %>
      <%= contact.value %>
    <% end %>
  </li>
</ul>

<%= link_to 'Volver', clients_path %>
